{
  "name": "gulp-starter",
  "version": "0.1.1",
  "description": "Gulp starter with common tasks and scenarios",
  "repository": {
    "type": "git",
    "url": "git://github.com/greypants/gulp-starter.git"
  },
  "browser": {
    "underscore": "backbone/node_modules/underscore",
    "plugin": "./src/javascript/vendor/jquery-plugin.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "^0.8.2",
    "browserify": "^3.36.0",
    "browserify-shim": "^3.4.1",
    "coffeeify": "^0.6.0",
    "connect": "^2.14.3",
    "event-stream": "^3.1.7",
    "gulp": "^3.6.0",
    "gulp-changed": "^0.3.0",
    "gulp-compass": "^1.1.8",
    "gulp-concat": "^2.3.4",
    "gulp-debug": "^0.3.0",
    "gulp-declare": "^0.2.0",
    "gulp-define-module": "^0.1.1",
    "gulp-handlebars": "^2.1.0",
    "gulp-imagemin": "^0.1.5",
    "gulp-notify": "^1.2.4",
    "gulp-open": "^0.2.8",
    "gulp-rename": "^1.2.0",
    "gulp-sourcemaps": "^1.1.0",
    "gulp-uglify": "^0.3.1",
    "gulp-util": "^2.2.14",
    "hbsfy": "^1.3.2",
    "pretty-hrtime": "^0.2.1",
    "vinyl-source-stream": "^0.1.1",
    "watchify": "^0.10.1"
  },
  "dependencies": {
    "backbone": "^1.1.2",
    "jquery": "^2.1.0"
  }
}
